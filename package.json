{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "postinstall": "remix setup cloudflare-pages",
    "esbuild:durables": "esbuild app/durables/* --outfile=durables/src/index.mjs --bundle",
    "wrangler:help": "wrangler paged dev help",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --watch ./build --kv TEST --do JOKES=JokesDurable@jokes-durable",
    "start": "npm run dev:wrangler"
  },
  "dependencies": {
    "@cloudflare/wrangler": "^1.19.7",
    "@remix-run/cloudflare-pages": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "esbuild": "0.13.14",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.2",
    "wrangler": "^0.0.0-ce61000"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
